{
	"name": "nuxt-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"preinstall": "npm run install:clean && npm install --ignore-scripts && npx npm-force-resolutions",
		"postinstall": "npm run lint",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\" ./.prettierrc",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path ./.eslintignore",
		"stylelint:fix": "stylelint \"./**/*.{css,scss,sass,vue}\" --fix --ignore-path ./.stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"test": "jest"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/style-resources": "^1.2.1",
		"core-js": "^3.27.2",
		"element-theme-default": "^1.4.13",
		"nuxt": "^2.15.8",
		"nuxt-element-ui": "^1.0.10",
		"vue": "^2.7.10",
		"vue-server-renderer": "^2.7.14",
		"vue-template-compiler": "^2.7.14",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/stylelint-module": "^4.1.0",
		"@vue/test-utils": "^1.3.0",
		"babel-jest": "^29.4.1",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"jest": "^29.4.1",
		"jest-environment-jsdom": "^29.4.1",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.3",
		"sass": "^1.57.1",
		"sass-loader": "^10.4.1",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-property-sort-order-smacss": "^9.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-sass-guidelines": "^9.0.1",
		"stylelint-config-standard": "^28.0.0",
		"vue-jest": "^3.0.7"
	},
	"resolutions": {
		"glob-parent": ">=5.1.2",
		"json5": ">=2.2.2",
		"nth-check": ">=2.0.1"
	}
}
