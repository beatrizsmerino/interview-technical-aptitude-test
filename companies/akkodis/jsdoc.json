{
	"source": {
		"include": [
			"./companies/akkodis/src"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"exclude": [
			"node_modules"
		]
	},
	"plugins": [
		"plugins/markdown"
	],
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"sourceType": "module",
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./companies/akkodis/jsdocs/",
		"recurse": true,
		"readme": "./companies/akkodis/README.md"
	}
}